<mvc:View controllerName="com.hse.wcintegration.controller.Main" displayBlock="true"
	xmlns="sap.m"
	xmlns:wc="sap.ui.webc.main"
	xmlns:mvc="sap.ui.core.mvc">

	<Page title="{i18n>appTitle}" text="{i18n>appTitle}" icon="sap-icon://accept" id="page" description="{i18n>appDescription}">
		<Button text="{i18n>btnText}" press="sayHello" />
		<wc:Label text="simple DateRangePicker" labelFor="DRP1"/>
		<wc:DateRangePicker id="DRP1" placeholder="Enter Date" change="handleChange" class="sapUiSmallMarginBottom"/>
	</Page>

</mvc:View>